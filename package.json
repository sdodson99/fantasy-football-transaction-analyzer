{
  "name": "fantasy-football-transaction-analyzer",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions,database,storage",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "start:emulators": "firebase emulators:start --only database,storage",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "fix:eslint": "eslint --fix \"src/**/*.ts\"",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/node": "^16.11.0",
    "axios": "^0.22.0",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "luxon": "^2.0.2",
    "playwright": "^1.15.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^2.7.3",
    "@types/luxon": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "jest": "^27.2.4",
    "jest-when": "^3.4.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "private": true
}
